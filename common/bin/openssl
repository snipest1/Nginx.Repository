#!/bin/sh
LD_LIBRARY_PATH="/home/tim/nginxstack-1.6.2-0/common/lib:$LD_LIBRARY_PATH"	   
DYLD_LIBRARY_PATH="/home/tim/nginxstack-1.6.2-0/common/lib:$DYLD_LIBRARY_PATH"	   
